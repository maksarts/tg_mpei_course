//https://leetcode.com/problems/sqrtx/

class Solution {
public:
    int mySqrt(int x) {
        return int(pow(x, 0.5));
        
    }
};

int stringToInteger(string input) {
    return stoi(input);
}

int main() {
    string line;
    while (getline(cin, line)) {
        int x = stringToInteger(line);
        
        int ret = Solution().mySqrt(x);

        string out = to_string(ret);
        cout << out << endl;
    }
    return 0;
}
